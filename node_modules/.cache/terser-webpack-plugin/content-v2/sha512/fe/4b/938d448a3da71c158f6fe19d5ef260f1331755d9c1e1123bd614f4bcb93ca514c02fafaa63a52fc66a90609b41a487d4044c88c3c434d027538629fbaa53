a = {\n        id: initial.id,\n        name: data.name,\n        productSetId: initialValues?.productSet?.id,\n        status: data?.status?.value,\n      };\n      setDisable(true);\n      const result = await categoryService.UpdateEmployee(customData);\n      if (result.isSuccess) {\n        toast.success(\"Cp nht loi sn phm thnh cng\");\n        history.push(\"/cms/classify/category/list\");\n      } else {\n        setDisable(false);\n      }\n      setDisable(false);\n    }\n  };\n\n  return (\n    <Row>\n      <Col sm=\"12\">\n        <Form onSubmit={handleSubmit(onSubmit)}>\n          <Row>\n            <Col sm=\"4\">\n              <Label for=\"nameVerticalIcons\">Tn loi sn phm:</Label>\n\n              <Controller\n                name=\"name\"\n                control={control}\n                render={({ field }) => (\n                  <InputGroup className=\"input-group-merge\" tag={FormGroup}>\n                    <Input\n                      className={classnames({ \"is-invalid\": errors[\"name\"] })}\n                      {...field}\n                    />\n                  </InputGroup>\n                )}\n              />\n              <p className=\"text-danger\">{errors.name?.message}</p>\n            </Col>\n            <Col sm=\"4\">\n              <Label for=\"nameVerticalIcons\">Tp sn phm:</Label>\n              <InputGroup className=\"input-group-merge\" tag={FormGroup}>\n                <Input\n                  disabled\n                  defaultValue={initialValues?.productSet?.name}\n                />\n              </InputGroup>\n            </Col>\n            <Col sm=\"4\">\n              <Label>Trng thi:</Label>\n              <Controller\n                control={control}\n                name=\"status\"\n                render={({ field }) => (\n                  <Select\n                    theme={selectThemeColors}\n                    isClearable={false}\n                    className=\"react-select\"\n                   